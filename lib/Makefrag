#if LAB >= 3

OBJDIRS += lib

LIB_SRCFILES :=		lib/console.c \
			lib/fd.c \
			lib/file.c \
			lib/fork.c \
			lib/fprintf.c \
			lib/fsipc.c \
			lib/ipc.c \
			lib/libos.c \
			lib/pageref.c \
			lib/pgfault.c \
			lib/pipe.c \
			lib/printf.c \
			lib/readline.c \
			lib/spawn.c \
			lib/string.c \
			lib/syscall.c \
			lib/wait.c
LIB_OBJFILES := $(patsubst %.c, %.o, $(LIB_SRCFILES))

lib/libuser.a: $(LIB_OBJFILES)
	@echo ar $@
	@$(AR) r $@ $(LIB_OBJFILES)

#endif

