ifdef LAB4

OBJDIRS += user

$(OBJDIR)/user/%: $(OBJDIR)/user/%.o $(OBJDIR)/lib/entry.o $(OBJDIR)/lib/libuser.a
	@echo ld $@
	@$(LD) -o $@ $(ULDFLAGS) $(LDFLAGS) -nostdlib \
		$(OBJDIR)/lib/entry.o $@.o -L$(OBJDIR)/lib -luser $(GCC_LIB)
	@$(OBJDUMP) --adjust-vma=0x7ff000 -S $@ > $@.asm
	@$(NM) -n $@ > $@.sym

$(OBJDIR)/user/%.o: lib.h

endif
